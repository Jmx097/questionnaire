{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "off",
    "react/no-unescaped-entities": "off",
    "prefer-const": "error",
    "no-var": "error",
    "eqeqeq": ["error", "always"],
    "curly": ["error", "multi-line"],
    "brace-style": ["error", "1tbs", { "allowSingleLine": true }]
  }
}
